<?xml version="1.0"?>
<odoo>
    <menuitem id="library_menu"
              name="Library" />

    <act_window id="action_library_book"
      name="Library Books"
      res_model="library.book"
      view_mode="tree,form"
    />

    <menuitem id="menu_library_book"
              name="Books"
              action="action_library_book"
              parent="library_menu" />
    

    <act_window id="action_library_categ"
      name="Categories"
      res_model="library.book.category"
      view_mode="tree,form"
    />

    <menuitem id="menu_library_categ"
              name="Categories"
              action="action_library_categ"
              parent="library_menu" />

    <act_window id="action_library_writers"
          name="Writers"
          res_model="library.book.writers"
          view_mode="tree,form"/>

    <menuitem id="menu_library_writer"
              name="Writers"
              action="action_library_writers"
              parent="library_menu" />
</odoo>
